ante",i18n_excited:"emoción",i18n_sparkle:"brillante",i18n_growing_heart:"corazón creciente",i18n_growing:"creciente",i18n_nervous:"nervioso",i18n_pulse:"latido",i18n_blue_heart:"corazón azul",i18n_green_heart:"corazón verde",i18n_green:"verde",i18n_yellow_heart:"corazón amarillo",i18n_yellow:"amarillo",i18n_orange_heart:"corazón naranja",i18n_orange:"naranja",i18n_purple_heart:"corazón morado",i18n_purple:"morado",i18n_black_heart:"corazón negro",i18n_black:"negro",i18n_wicked:"corazón",i18n_heart_with_ribbon:"corazón con lazo",i18n_ribbon:"lazo",i18n_valentine:"valentín",i18n_revolving_hearts:"corazones giratorios",i18n_revolving:"giratorio",i18n_heart_decoration:"adorno de corazón",i18n_heavy_heart_exclamation:"exclamación de corazón",i18n_exclamation:"exclamación",i18n_mark:"corazón",i18n_punctuation:"puntuación",i18n_love_letter:"carta de amor",i18n_letter:"carta",i18n_mail:"correo",i18n_anger_symbol:"símbolo de enfado",i18n_bomb:"bomba",i18n_collision:"colisión",i18n_boom:"cómic",i18n_sweat_droplets:"gotas de sudor",i18n_splashing:"sudor",i18n_dashing_away:"salir corriendo",i18n_dash:"correr",i18n_speech_balloon:"bocadillo de diálogo",i18n_balloon:"bocadillo",i18n_bubble:"burbuja",i18n_dialog:"diálogo",i18n_speech:"conversación",i18n_left_speech_bubble:"bocadillo de diálogo por la izquierda",i18n_right_anger_bubble:"bocadillo de enfado por la derecha",i18n_thought_balloon:"bocadillo de pensamiento",i18n_thought:"pensamiento",i18n_hole:"agujero",i18n_eyeglasses:"accesorios",i18n_dark:"oscuro",i18n_necktie:"corbata",i18n_tie:"accesorio","i18n_t-shirt":"camiseta",i18n_shirt:"ropa",i18n_tshirt:"camisa",i18n_jeans:"vaqueros",i18n_pants:"pantalones",i18n_trousers:"ropa",i18n_scarf:"bufanda",i18n_neck:"cuello",i18n_gloves:"guantes",i18n_coat:"abrigo",i18n_jacket:"chaquetón",i18n_socks:"calcetines",i18n_stocking:"pies",i18n_dress:"vestido",i18n_kimono:"kimono",i18n_bikini:"bikini",i18n_womans_clothes:"ropa de mujer",i18n_purse:"monedero",i18n_coin:"cartera",i18n_handb